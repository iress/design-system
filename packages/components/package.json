{
  "name": "@iress-oss/ids-components",
  "version": "6.0.0-alpha.3",
  "description": "Iress React Component Library",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/src/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "yarn panda:prepare && vite build",
    "build-storybook": "storybook build --stats-json",
    "clean": "rimraf dist coverage src/styled-system",
    "dev": "concurrently \"yarn panda:watch\" \"yarn storybook\"",
    "dev:no-open": "concurrently \"yarn panda:watch\" \"yarn storybook --no-open\"",
    "lint": "eslint . --max-warnings 0",
    "panda:prepare": "panda cssgen && panda codegen",
    "panda:watch": "panda --watch",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --config vitest.config.ts --watch",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "test:ui": "vitest --config vitest.config.ts --ui",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json"
  },
  "devDependencies": {
    "@iress-oss/ids-tokens": "^6.0.0-alpha.2",
    "@pandacss/dev": "1.5.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest-axe": "3.5.9",
    "@types/query-selector-shadow-dom": "1.0.4",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "4.0.10",
    "@vitest/ui": "4.0.10",
    "concurrently": "^9.2.1",
    "eslint": "9.39.1",
    "eslint-plugin-file-progress": "^3.0.2",
    "eslint-plugin-mdx": "3.6.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-sonarjs": "^3.0.5",
    "glob": "12.0.0",
    "jest-axe": "10.0.0",
    "jsdom": "27.2.0",
    "lightningcss": "^1.30.2",
    "postcss": "8.4.31",
    "postcss-preset-env": "10.3.1",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.1.1",
    "react-dropzone": "14.3.8",
    "rimraf": "^6.1.2",
    "storybook": "10.0.8",
    "typescript": "5.9.3",
    "vite": "7.2.4",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-static-copy": "3.1.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "4.0.10"
  },
  "dependencies": {
    "@floating-ui/react": "0.27.16",
    "@fortawesome/fontawesome-common-types": "0.2.36",
    "@tanstack/react-table": "8.21.3",
    "@types/mdx": "^2.0.13",
    "fuzzysort": "3.1.0",
    "query-selector-shadow-dom": "1.0.1",
    "react-hook-form": "7.55.0",
    "use-debounce": "10.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iress/design-system"
  },
  "stableVersion": "6.0.0-alpha.2"
}
