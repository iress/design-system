{
  "name": "@iress-oss/ids-components",
  "version": "6.0.0-alpha.0",
  "description": "Iress React Component Library",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/src/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "yarn panda:prepare && vite build",
    "clean": "rimraf dist coverage src/styled-system",
    "dev": "concurrently \"yarn panda:watch\" \"vite build --watch\"",
    "lint": "eslint . --max-warnings 0",
    "panda:prepare": "panda cssgen && panda codegen",
    "panda:watch": "panda --watch",
    "test": "vitest run --config vitest.config.ts --coverage",
    "test:ui": "vitest --config vitest.config.ts --ui",
    "test:watch": "vitest --config vitest.config.ts --watch",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "devDependencies": {
    "@iress-oss/ids-tokens": "6.0.0-alpha.0",
    "@pandacss/dev": "1.3.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest-axe": "3.5.9",
    "@types/query-selector-shadow-dom": "1.0.4",
    "@types/react": "18.3.24",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react": "5.0.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "9.35.0",
    "eslint-plugin-file-progress": "3.0.2",
    "eslint-plugin-mdx": "3.6.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react-hooks": "^6.0.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-sonarjs": "^3.0.5",
    "glob": "10.3.10",
    "jest-axe": "10.0.0",
    "jsdom": "26.1.0",
    "postcss": "8.4.31",
    "postcss-preset-env": "10.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "14.3.8",
    "rimraf": "^6.0.1",
    "typescript": "5.8.2",
    "vite": "7.1.5",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-static-copy": "3.1.2",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@floating-ui/react": "0.26.28",
    "@fortawesome/fontawesome-common-types": "0.2.36",
    "@tanstack/react-table": "8.21.3",
    "fuzzysort": "3.1.0",
    "query-selector-shadow-dom": "1.0.1",
    "react-hook-form": "7.55.0",
    "use-debounce": "10.0.6"
  }
}
