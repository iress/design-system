{
  "name": "@iress-oss/ids-tokens",
  "version": "6.0.0-alpha.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./mapTokensToCssVariables": "./dist/helpers/mapTokensToCssVariables.js"
  },
  "files": [
    "build",
    "dist"
  ],
  "scripts": {
    "analyse": "yarn vite-bundle-visualizer",
    "build": "yarn cssVars && vite build",
    "clean": "rimraf build dist coverage src/generated",
    "cssVars": "tsx plugins/generateCssVars.ts",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx,.mjs,.cjs,.mdx --max-warnings=0",
    "test": "vitest run --config vitest.config.ts --coverage",
    "test:watch": "vitest --config vitest.config.ts --watch",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "watch": "vite build --watch"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitest/coverage-v8": "3.2.4",
    "chalk": "5.6.2",
    "colorjs.io": "0.5.2",
    "eslint": "9.35.0",
    "eslint-plugin-file-progress": "^3.0.2",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-prettier": "^5.5.4",
    "jest-axe": "10.0.0",
    "jsdom": "26.1.0",
    "json5": "1.0.2",
    "radash": "12.1.1",
    "rimraf": "6.0.1",
    "rollup-plugin-tree-shakeable": "2.0.0",
    "style-dictionary": "5.0.4",
    "tsx": "4.20.5",
    "typescript": "5.8.2",
    "vite": "7.1.5",
    "vite-bundle-visualizer": "1.2.1",
    "vite-plugin-dts": "4.5.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "radash": "^12.1.0",
    "style-dictionary": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "radash": {
      "optional": true
    },
    "style-dictionary": {
      "optional": true
    }
  }
}
