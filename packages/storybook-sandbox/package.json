{
  "name": "@iress-oss/ids-storybook-sandbox",
  "version": "0.0.0",
  "description": "A Storybook addon that provides a sandbox environment for testing and experimenting with components.",
  "keywords": [
    "storybook-addons"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "author": "Iress",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "concurrently \"yarn watch\" \"yarn serve\"",
    "test": "vitest --config vitest.config.ts --watch",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve": "yarn storybook:serve-addon packages/storybook-sandbox",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@storybook/icons": "^1.4.0",
    "react-live": "^4.1.8"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vueless/storybook-dark-mode": "^9.0.9",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.64.0",
    "storybook": "^9.1.0",
    "tsup": "^8.2.4",
    "typescript": "^5.8.2",
    "vite": "^7.1.7",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@vueless/storybook-dark-mode": "^9.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "storybook": "^9.0.0"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.tsx"
    ]
  },
  "storybook": {
    "displayName": "Storybook Sandbox",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
