{
  "name": "@iress-oss/ids-storybook-toggle-stories",
  "version": "0.0.1",
  "description": "A Storybook addon that adds a toolbar button to toggle between different story variants.",
  "keywords": [
    "storybook-addons"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "author": "Iress",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "concurrently \"yarn watch\" \"yarn serve\"",
    "dev:disabled": "DISABLE_ADDON=true yarn dev",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx,.mjs,.cjs,.mdx --max-warnings=0",
    "test": "vitest --config vitest.config.ts --watch",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "serve": "yarn storybook:serve-addon packages/storybook-toggle-stories",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@storybook/icons": "^2.0.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "4.0.9",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-file-progress": "^3.0.2",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^10.0.8",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "4.0.9"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "storybook": "^9.0.0"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ]
  },
  "storybook": {
    "displayName": "Storybook Toggle Stories",
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iress/design-system"
  }
}
