{
  "name": "@iress-oss/ids-storybook-okta",
  "version": "0.0.0",
  "description": "Protect your Storybook instance(s) using OKTA",
  "keywords": [
    "storybook-addons"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "author": "Iress",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "concurrently \"yarn watch\" \"yarn serve\"",
    "dev:auth": "AUTH=1 concurrently \"yarn watch\" \"yarn serve\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve": "yarn storybook:serve-addon packages/storybook-okta",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@okta/okta-auth-js": "^7.14.0",
    "@storybook/icons": "^1.4.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "@types/node": "^22",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^9.1.0",
    "tsup": "^8.2.4",
    "typescript": "^5.8.2",
    "vite": "^7.1.7"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "storybook": "^9.0.0"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.tsx"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "OKTA Authentication",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
