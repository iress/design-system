{
  "name": "@iress-oss/ids-storybook-okta",
  "version": "0.1.1",
  "description": "Protect your Storybook instance(s) using OKTA",
  "keywords": [
    "storybook-addons"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "author": "Iress",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js"
    },
    "./preset": "./dist/preset.js",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "yarn clean && tsup",
    "clean": "rimraf dist",
    "dev": "concurrently \"yarn watch\" \"yarn serve\"",
    "dev:auth": "AUTH=1 concurrently \"yarn watch\" \"yarn serve\"",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx,.mjs,.cjs,.mdx --max-warnings=0",
    "test": "vitest --config vitest.config.ts --watch",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "serve": "yarn storybook:serve-addon packages/storybook-okta",
    "watch": "tsup --watch"
  },
  "dependencies": {
    "@okta/okta-auth-js": "^7.14.0",
    "@storybook/icons": "2.0.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "10.0.8",
    "@storybook/react-vite": "10.0.8",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "4.0.10",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-file-progress": "^3.0.2",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.3.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "6.1.0",
    "storybook": "10.0.8",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "7.2.2",
    "vitest": "4.0.10"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "storybook": "^10"
  },
  "bundler": {
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.tsx",
      "src/index.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "OKTA Authentication",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iress/design-system"
  }
}
