{
  "name": "@iress-oss/ids-storybook-config",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./main": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./manager": {
      "types": "./dist/manager.d.ts",
      "import": "./dist/manager.js",
      "require": "./dist/manager.cjs"
    }
  },
  "files": [
    "dist",
    "public"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "analyse": "yarn vite-bundle-visualizer",
    "build": "vite build",
    "dev": "yarn storybook",
    "clean": "rimraf dist coverage storybook-static",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx,.mjs,.cjs,.mdx --max-warnings=0",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --config vitest.config.ts --watch",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@iress-oss/ids-tokens": "^6.0.0-alpha.1",
    "react-diff-viewer": "^3.1.1",
    "react-element-to-jsx-string": "^17.0.1",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@iress-oss/ids-storybook-okta": "^0.0.0",
    "@iress-oss/ids-storybook-sandbox": "^0.0.0",
    "@iress-oss/ids-storybook-toggle-stories": "^0.0.0",
    "@iress-oss/ids-storybook-version-badge": "^0.0.0",
    "@storybook/addon-a11y": "^9.0.0",
    "@storybook/addon-docs": "^9.0.0",
    "@storybook/addon-links": "^9.1.13",
    "@storybook/react-vite": "^9.1.10",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19",
    "@types/react-is": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vueless/storybook-dark-mode": "^9.0.0",
    "chalk": "^5.6.2",
    "colorjs.io": "^0.5.2",
    "eslint": "^9.39.1",
    "eslint-plugin-file-progress": "^3.0.2",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-prettier": "^5.5.4",
    "happy-dom": "^20.0.8",
    "jsdom": "^27.1.0",
    "json5": "^2.2.3",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-is": "^19.2.0",
    "rimraf": "^6.1.0",
    "rollup-plugin-tree-shakeable": "^2.0.0",
    "storybook": "^9.0.0",
    "storybook-addon-tag-badges": "^2.0.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.1",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@iress-oss/ids-components": "^6.0.0-alpha.1",
    "@iress-oss/ids-storybook-okta": "^0.0.0",
    "@iress-oss/ids-storybook-sandbox": "^0.0.0",
    "@iress-oss/ids-storybook-toggle-stories": "^0.0.0",
    "@iress-oss/ids-storybook-version-badge": "^0.0.0",
    "@storybook/addon-a11y": "^9.0.0",
    "@storybook/addon-docs": "^9.0.0",
    "@storybook/react-vite": "^9.1.10",
    "@vueless/storybook-dark-mode": "^9.0.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "storybook": "^9.0.0",
    "storybook-addon-tag-badges": "^2.0.2",
    "vite": "^7.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iress/design-system"
  }
}
